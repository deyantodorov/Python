   "source": [
    "import time\n",
    "\n",
    "start = time.time()\n",
    "\n",
    "total_sum = 0\n",
    "\n",
    "for i in range(100000000):\n",
    "    total_sum += i\n",
    "\n",
    "print(f'Sum: {total_sum}')\n",
    "print(f'For loop: {time.time() - start} seconds.')"
